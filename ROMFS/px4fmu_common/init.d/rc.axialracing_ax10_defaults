#!nsh

set VEHICLE_TYPE rover
echo "rc.axialracing_ax_default"

# This section can be enabled once tuning parameters for this particular
# rover model are known. It allows to configure default gains via the GUI
	 param set CBRK_BUZZER 782097
	 param set CBRK_IO_SAFETY 22027
	 param set CBRK_AIRSPD_CHK 162128
	 param set COM_RC_IN_MODE 1
	 param set NAV_ACC_RAD 1
	 param set CBRK_USB_CHK 197848
	 param set TRIG_MODE 1
     param set TRIG_ACT_TIME 300
     param set TRIG_PINS 12
	 param set TRIG_POLARITY 1
	 param set TRIG_INTERVAL 0
	 param set SYS_COMPANION 921600


